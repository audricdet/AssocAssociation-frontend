{
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ],
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "https://assoc-associations.vercel.app, http://localhost:5173"
          },
          {
            "key": "Access-Control-Allow-Credentials",
            "value": "true"
          }
        ]
  }
